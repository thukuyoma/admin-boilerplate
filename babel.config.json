{
    "presets": ["next/babel"],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./"],
          "alias": {
            "@components": "./src/components",
            "@api": "./src/api",
            "@models": "./src/models",
            "@screens": "./src/screens",
            "@hooks": "./src/hooks",
            "@services": "./src/services",
            "@constants": "./src/constants",
            "@context": "./src/context",
            "@queries": "./src/queries",
            "@data": "./src/data",
            "@typeDefs": "./src/types",
            "@generated": "./src/generated",
            "@utils": "./src/utils",
            "@public": "./public"
          }
        }
      ], ["styled-components", { "ssr": true }], "inline-react-svg"
    ]
  }